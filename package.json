{
  "name": "useful-decorators",
  "description": "Some useful JavaScript decorators",
  "version": "0.4.1",
  "author": "andres-brugarolas",
  "license": "GNU v3",
  "private": false,
  "type": "module",
  "scripts": {
    "test": "npm run build:lib && npm run build:test && mocha ./dist/test --require @babel/register",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint --fix ./src ./test",
    "clean": "rimraf ./dist",
    "build:lib": "npm run clean && babel ./src --out-dir ./dist --copy-files",
    "build:test": "babel ./test --out-dir ./dist/test --copy-files",
    "utils:outdated": "npm outdated",
    "utils:updates": "ncu",
    "utils:upgrades": "ncu -u"
  },
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "dist/after.js",
    "dist/autobind.js",
    "dist/before.js",
    "dist/bind.js",
    "dist/debounce.js",
    "dist/decorate.js",
    "dist/defer.js",
    "dist/delay.js",
    "dist/immutable.js",
    "dist/inject.js",
    "dist/interval.js",
    "dist/lazy.js",
    "dist/memoize.js",
    "dist/mixin.js",
    "dist/observe.js",
    "dist/once.js",
    "dist/provide.js",
    "dist/singleton.js",
    "dist/throttle.js"
  ],
  "dependencies": {
    "@babel/runtime-corejs3": "^7.21.0",
    "lodash.debounce": "^4.0.8",
    "lodash.defer": "^4.1.0",
    "lodash.delay": "^4.1.1",
    "lodash.memoize": "^4.1.2",
    "lodash.once": "^4.1.1",
    "lodash.throttle": "^4.1.1",
    "on-change": "^4.0.2"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "browserslist": [
    ">0.25%",
    "not dead",
    "not IE > 5",
    "not android < 5"
  ],
  "keywords": [
    "javascript",
    "babel",
    "decorators",
    "annotations",
    "aspects"
  ],
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "babel-preset-minify": "^0.5.2",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-canonical": "^40.0.9",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.7.12",
    "rimraf": "^4.4.0",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brugarolas/useful-decorators"
  }
}
