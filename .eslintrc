{
  "root": true,
  "extends": [
    "canonical"
  ],
  "rules": {
    "@babel/object-curly-spacing": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "linebreak-style": 0,
    "import/extensions": 0,
    "unicorn/prefer-node-remove": 0,
    "unicorn/prefer-node-append": 0,
    "object-curly-newline": ["error", { "multiline": true }]
  }
}
